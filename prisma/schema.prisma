generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model delete_me {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model users {
  id       String   @id @default(auto()) @map("_id") @db.ObjectId
  email    String
  name     String
  password String
  roles    String[]
}
